iv>\n\n<!-- isMarrie:true oldugunda --- Mutluluklar!!! -->\n<div v-if= \"isMarried\">Mutluluklar!!! </div> \n<!-- isMarrie:false oldugunda --- En güzel zamanlara... -->\n<div v-else>En güzel zamanlara...</div> \n\n    <!-- v-for -->\n    <div>\n      <ul>\n        <li v-for=\"course in courses\" :key=\"course\">\n        {{ course }}\n        </li>\n      </ul>\n    </div>\n<!-- ayni sekilde key propertisine index atama ile yapma -->\n    <div>\n      <ul>\n        <li v-for=\"(course, index) in courses\" :key=\"index\">\n        {{ course }}\n        </li>\n      </ul>\n    </div>\n\n  <!-- v-model v-bind a göre daha güclü-->\n\n  <div>\n    Kullanici adi <br/>\n    <input type=\"text\" v-model=\"user.userName\" /><br/>\n    Kullanici sifresi <br/>\n    <input type=\"text\" v-model=\"user.userPassword\"/><br/>\n    onaylama sifresi <br/>\n    <input type=\"text\" v-model=\"user.confirmPassword\" /><br/>\n    <div class=\"liste\" v-if=\"user.userPassword !== user.confirmPassword\">sifreler uyumsuz</div>\n<div><button :disabled=\"!user.userName || user.userPassword !== user.confirmPassword\">Gönder</button></div>\n  </div>\n\n\n<!-- watch uygulamasi -->\n  <div>\n    <h2>INFOTECH GPT</h2>\n      <input type=\"text\" v-model=\"soru\"/>\n      <div>{{ cevap }}</div>\n  </div>\n\n  </div>\n</template>\n\n<script>\nexport default {\n  data () {\n   return {\n    name:\"mehmet\",\n    surName: \"yilmaz\",\n    age: 55,\n    courses: [\"nodejs\",\"css\",\"js\",\"vuejs\"],\n    user: {\n      userName:\"\",\n      userPassword:\"\",\n      confirmPassword:\"\"\n    },\n    counter: 0,\n    inputValue: \"Click me!\",\n    isMarried: false,\n    soru:\"\",\n    cevap:\"\"\n   } \n  },\n  computed:{\n      fullName () {\n        return`${this.name} ${this.surName}----> ${this.age}`\n      }\n  },\n  methods:{\n    arttir(){\n      this.counter++;\n    },\n    azalt(){\n      this.counter--;\n    }\n  },\n  watch:{\n    soru (yeniDeger, eskiDeger){\n      console.log(\"Yenideger\", yeniDeger);\n      console.log(\"Eskideger\", eskiDeger);\n    }\n  }\n}\n</script>\n\n<style>\n.liste{background-color: blanchedalmond;}\n</style>\n"],"mappings":"AAyEA,eAAe;EACbA,IAAGA,CAAA,EAAK;IACP,OAAO;MACNC,IAAI,EAAC,QAAQ;MACbC,OAAO,EAAE,QAAQ;MACjBC,GAAG,EAAE,EAAE;MACPC,OAAO,EAAE,CAAC,QAAQ,EAAC,KAAK,EAAC,IAAI,EAAC,OAAO,CAAC;MACtCC,IAAI,EAAE;QACJC,QAAQ,EAAC,EAAE;QACXC,YAAY,EAAC,EAAE;QACfC,eAAe,EAAC;MAClB,CAAC;MACDC,OAAO,EAAE,CAAC;MACVC,UAAU,EAAE,WAAW;MACvBC,SAAS,EAAE,KAAK;MAChBC,IAAI,EAAC,EAAE;MACPC,KAAK,EAAC;IACP;EACD,CAAC;EACDC,QAAQ,EAAC;IACLC,QAAOA,CAAA,EAAK;MACV,OAAO,GAAE,IAAI,CAACd,IAAI,IAAI,IAAI,CAACC,OAAQ,SAAQ,IAAI,CAACC,GAAI;IACtD;EACJ,CAAC;EACDa,OAAO,EAAC;IACNC,MAAMA,CAAA,EAAE;MACN,IAAI,CAACR,OAAO,EAAE;IAChB,CAAC;IACDS,KAAKA,CAAA,EAAE;MACL,IAAI,CAACT,OAAO,EAAE;IAChB;EACF,CAAC;EACDU,KAAK,EAAC;IACJP,IAAGA,CAAGQ,SAAS,EAAEC,SAAS,EAAC;MACzBC,OAAO,CAACC,GAAG,CAAC,WAAW,EAAEH,SAAS,CAAC;MACnCE,OAAO,CAACC,GAAG,CAAC,WAAW,EAAEF,SAAS,CAAC;IACrC;EACF;AACF"},"metadata":{},"sourceType":"module","externalDependencies":[]}